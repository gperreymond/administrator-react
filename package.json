  },
  "devDependencies": {
    "babel": "5.4.7",
    "babel-core": "5.4.7",
    "babel-loader": "5.1.3",
    "copy-webpack-plugin": "2.1.3",
    "css-loader": "0.23.1",
    "npm-run-all": "1.8.0",
    "postcss-loader": "0.9.1",
    "riot": "2.4.0",
    "riot-mui": "0.0.15",
    "riotcontrol": "0.0.3",
    "riotjs-loader": "3.0.0",
    "style-loader": "0.13.1",
    "webpack": "2.0.1-beta",
    "webpack-dev-server": "1.14.1"
  }
}
