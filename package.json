{
  "name": "riotjs-starterkit",
  "version": "1.0.0",
  "description": "A riotjs Webpack starterkit from abibao.com",
  "main": "webpack.config.js",
  "scripts": {
    "start:dev": "clear && export NODE_ENV=development && npm-run-all task:bundle task:serve",
    "task:bundle": "webpack --progress --colors --optimize-minimize",
    "task:serve": "webpack-dev-server --hot --inline"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel": "6.5.2",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "copy-webpack-plugin": "2.1.3",
    "npm-run-all": "1.8.0",
    "riot": "2.4.0",
    "riotcontrol": "0.0.3",
    "riotjs-loader": "3.0.0",
    "webpack": "2.0.1-beta",
    "webpack-dev-server": "1.14.1"
  }
}
