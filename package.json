{
  "name": "riotjs-starterkit",
  "version": "1.0.0",
  "description": "A riotjs Webpack starterkit from abibao.com",
  "main": "webpack.config.js",
  "scripts": {
    "start:dev": "clear && export NODE_ENV=development && npm-run-all task:bundle task:serve",
